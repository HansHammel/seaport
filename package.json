{
  "name": "seaport",
  "description": "service registry and port assignment for clusters",
  "version": "2.0.10",
  "repository": {
    "type": "git",
    "url": "git://github.com/substack/seaport.git"
  },
  "main": "index.js",
  "browser": "lib/seaport.js",
  "bin": {
    "seaport": "bin/cmd.js"
  },
  "keywords": [
    "port",
    "allocate",
    "hub",
    "service",
    "registry",
    "roles"
  ],
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "test": "tap test/*.js"
  },
  "dependencies": {
    "defined": "latest",
    "indexof": "latest",
    "inherits": "latest",
    "merge": "latest",
    "object-keys": "latest",
    "optimist": "latest",
    "readable-stream": "latest",
    "secure-peer": "latest",
    "semver": "latest",
    "split": "latest",
    "stream-combiner": "latest",
    "through": "latest"
  },
  "engine": {
    "node": ">=0.8"
  },
  "devDependencies": {
    "async": "^2",
    "destroyer": "latest",
    "macgyver": "latest",
    "tap": "^0.4"
  },
  "license": "MIT",
  "author": {
    "name": "James Halliday",
    "email": "mail@substack.net",
    "url": "http://substack.net"
  }
}
